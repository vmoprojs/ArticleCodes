  weighted=0#
  maxdist=2#
  ### eucliden likelihood ################ #
  res1=STBEUFit(theta,fix,coords,times,cc,data,type_dist,maxdist ,maxtime,winc,winstp,0,0,type_subs,weighted)#
  eu_par1=c(res1$par[1],res1$par[3],res1$par[4],res1$par[2])#
